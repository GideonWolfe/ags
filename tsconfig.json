{
    "compilerOptions": {
        "target": "ES2022",
        "moduleDetection": "force",
        "module": "ES2022",
        "lib": [
            "ES2022"
        ],
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "noImplicitAny": false,
        "baseUrl": ".",
        "typeRoots": [
            "./types"
        ],
        "skipLibCheck": true,
        "allowImportingTsExtensions": true
    }
}
